{
  "name": "IoTSmartSys MCB R1",
  "vendor": "IoTSmartSys",
  "url": "https://example.local/iotsmartsys/mcb",
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "platform": "espressif32",
  "build": {
    "mcu": "esp32",
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "flash_freq": "40m",
    "flash_size": "4MB",
    "core": "esp32",
    "variant": "esp32",
    "extra_flags": [
      "-DIOTSMARTSYS_BOARD_ID=\"iotsmartsys_mcb_r1\"",
      "-DIOTSMARTSYS_MCB01=1",
      "-DIOTSMARTSYS_BOARD_REV=1"
    ]
  },
  "upload": {
    "protocol": "esptool",
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 1310720,
    "speed": 460800
  },
  "debug": {
    "default_tool": "esp-prog"
  }
}